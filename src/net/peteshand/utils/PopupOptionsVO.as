package net.peteshand.utils {	/**	 * ...	 * @author Pete Shand	 */	public class PopupOptionsVO	{		public var channelmode:Boolean = false;		public var directories:Boolean = true;		public var fullscreen:Boolean = false;		public var location:Boolean = true;		public var menubar:Boolean = true;		public var resizable:Boolean = true;		public var scrollbars:Boolean = true;		public var titlebar:Boolean = true;		public var toolbar:Boolean = true;				public var height:int = -1;		public var width:int = -1;				public var x:int = -1;		public var y:int = -1;				public var align:String = '';				public var callback:Function;				public static function info(variable:String):String		{			var returnText:XML = new XML();			if (variable == "channelmode") {				returnText =				<![CDATA[					Options: yes | no | 1 | 0					info: Whether or not to display the window in theater mode. Default is no. IE only				]]>;			}			else if (variable == "directories") {				returnText =				<![CDATA[					Options: yes | no | 1 | 0					info: Whether or not to add directory buttons. Default is yes. IE only				]]>;			}			else if (variable == "fullscreen") {				returnText =				<![CDATA[					Options: yes | no | 1 | 0					info: Whether or not to display the browser in full-screen mode. Default is no. A window in full-screen mode must also be in theater mode. IE only				]]>;			}			else if (variable == "height") {				returnText =				<![CDATA[					Options: int (pixels)					info: The height of the window. Min. value is 100				]]>;			}			else if (variable == "left") {				returnText =				<![CDATA[					Options: int (pixels)					info: The left position of the window				]]>;			}			else if (variable == "location") {				returnText =				<![CDATA[					Options: yes | no | 1 | 0					info: Whether or not to display the address field. Default is yes				]]>;			}			else if (variable == "menubar") {				returnText =				<![CDATA[					Options: yes | no | 1 | 0					info: Whether or not to display the menu bar. Default is yes				]]>;			}			else if (variable == "resizable") {				returnText =				<![CDATA[					Options: yes | no | 1 | 0					info: Whether or not the window is resizable. Default is yes				]]>;			}			else if (variable == "scrollbars") {				returnText =				<![CDATA[					Options: yes | no | 1 | 0					info: Whether or not to display scroll bars. Default is yes				]]>;			}			else if (variable == "status") {				returnText =				<![CDATA[					Options: yes | no | 1 | 0					info: Whether or not to add a status bar. Default is yes				]]>;			}			else if (variable == "titlebar") {				returnText =				<![CDATA[					Options: yes | no | 1 | 0					info: Whether or not to display the title bar. Ignored unless the calling application is an HTML Application or a trusted dialog box. Default is yes				]]>;			}			else if (variable == "toolbar") {				returnText =				<![CDATA[					Options: yes | no | 1 | 0					info: Whether or not to display the browser toolbar. Default is yes				]]>;			}			else if (variable == "top") {				returnText =				<![CDATA[					Options: int (pixels)					info: The top position of the window. IE only				]]>;			}			else if (variable == "width") {				returnText =				<![CDATA[					Options:int (pixels)					info: The width of the window. Min. value is 100				]]>;			}						return returnText;		}	}}